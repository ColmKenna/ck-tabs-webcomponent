{
  "name": "@colmkenna/ck-tabs",
  "version": "1.0.4",
  "description": "A web component library featuring ck-tabs.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "unpkg": "dist/index.esm.js",
  "jsdelivr": "dist/index.esm.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "serve": "http-server dist -p 8080 -o",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --config ./jest.config.cjs",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\""
  },
  "keywords": [
    "web-components",
    "custom-elements",
    "javascript",
    "library"
  ],
  "sideEffects": false,
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ColmKenna/ck-tabs-webcomponent.git"
  },
  "bugs": {
    "url": "https://github.com/ColmKenna/ck-tabs-webcomponent/issues"
  },
  "homepage": "https://github.com/ColmKenna/ck-tabs-webcomponent#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "prettier": "^2.8.8"
  }
}
